    "StringParameter": {
      "type": "string"
    },
    "BoolParameter": {
      "type": "bool",
      "defaultValue": "false"
    },
    "IntParameter": {
      "type": "int",
      "defaultValue": "258"
    },
    "DecimalParameter": {
      "type": "decimal",
      "defaultValue": "25.3"
    },
    "EnumParameter": {
      "type": "enum",
      "values": {
        "EnumValue1": "",
        "EnumValue2": "",
        "EnumValue3": ""
      },
      "defaultValue": "EnumValue2"
    }
  }
