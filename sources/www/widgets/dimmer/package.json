{
  "type": "dimmer",
  "version": "1.0.2",
  "author": "Jean-Michel BERHAULT",
  "url": "https://github.com/Yadoms/yadoms/wiki/dimmer",
  "credits": "",
  "dependencies": {
    "yadoms": {
      "minimumVersion": "2.2.0"
    }
  },
  "dimensions": {
    "min": {
      "x": 1,
      "y": 1
    },
    "max": {
      "x": 4,
      "y": 4
    },
    "default": {
      "x": 2,
      "y": 2
    }
  },
  "hasTitle": true,
  "configurationSchema": {
    "device": {
      "type": "keyword",
      "expectedKeywordType": "numeric",
      "expectedKeywordAccess": "getset"
    },
    "StepValue": {
      "type": "decimal",
      "defaultValue": "0.2"
    }
  }
}